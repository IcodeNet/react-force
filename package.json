{
  "name": "@nareshbhatia/react-force",
  "description": "Foundational components for React applications",
  "version": "0.0.0-development",
  "author": "Naresh Bhatia",
  "license": "MIT",
  "homepage": "https://github.com/nareshbhatia/react-force",
  "repository": {
    "type": "git",
    "url": "https://github.com/nareshbhatia/react-force.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "material-ui",
    "react"
  ],
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "src/",
    "dist/"
  ],
  "scripts": {
    "build": "yarn lint && yarn build-src && yarn build-declaration",
    "build-src": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps",
    "build-declaration": "tsc --emitDeclarationOnly",
    "build-storybook": "build-storybook",
    "cz-commit": "git-cz",
    "format": "prettier --write README.md 'src/**/*.ts*'",
    "lint": "eslint 'src/**/*.ts*'",
    "prebuild": "rimraf dist",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "semantic-release": "semantic-release",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "test:prod": "jest --coverage --no-cache",
    "test:watch": "jest --watch"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.x",
    "classnames": "^2.x",
    "react": "^16.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@material-ui/core": "^4.0.1",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-links": "^5.0.11",
    "@storybook/addons": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "@types/classnames": "^2.2.7",
    "@types/jest": "^24.0.11",
    "@types/node": "^12.0.0",
    "@types/react": "^16.8.10",
    "@types/react-dom": "^16.8.3",
    "@types/storybook__react": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^1.8.0",
    "@typescript-eslint/parser": "^1.8.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "classnames": "^2.2.6",
    "colors": "^1.3.3",
    "commitizen": "^3.0.7",
    "coveralls": "^3.0.3",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "glob-loader": "^0.3.0",
    "husky": "^2.2.0",
    "jest": "^24.5.0",
    "jest-dom": "^3.2.1",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "prompt": "^1.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "react-testing-library": "^7.0.0",
    "replace-in-file": "^4.0.0",
    "rimraf": "^2.6.3",
    "semantic-release": "^15.13.3",
    "typescript": "^3.4.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "analyzeCommits": {
      "preset": "angular",
      "releaseRules": [
        {
          "type": "docs",
          "scope": "README",
          "release": "patch"
        },
        {
          "type": "refactor",
          "release": "patch"
        },
        {
          "type": "style",
          "release": "patch"
        }
      ]
    }
  },
  "jest": {
    "verbose": true,
    "roots": [
      "<rootDir>/src"
    ],
    "testRegex": "(\\.(test|spec))\\.(ts|tsx)$",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupFilesAfterEnv": [
      "./src/test/setupTestsAfterEnv.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 70,
        "lines": 80,
        "statements": 80
      }
    }
  }
}
